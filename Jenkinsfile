                        echo 'Dracarys'
                  }
            }
            stage('Build') {
                  steps {
                        echo 'Building Sample Maven Project'
                  }
            }
            stage('Deploy') {
                  steps {
                        echo "Dracarys in Staging Area"
                  }
            }
            stage('Deploy Production') {
                  steps {
                        echo "Dracarys in Production Area"
                  }
            }
      }
}
